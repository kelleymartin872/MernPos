\r\n            }\r\n\r\n            case \"POST\":\r\n            { \r\n                return new Promise( function(resolve,reject)\r\n                {\r\n                    axios.post(reqUrl, reqObj)\r\n                        .then(res => {\r\n                            window.serverData = res.data;\r\n                            resolve(window.serverData);\r\n                        })\r\n                        .catch(err => {\r\n                            if(err.response && err.response.data)\r\n                                window.serverData = err.response.data;\r\n                            else\r\n                            {\r\n                                window.serverData = {\r\n                                    data:\r\n                                    {\r\n                                        isOffline:true,\r\n                                        errorMsg:\"Server is Unreachable!\"\r\n                                    }\r\n                                };\r\n                            }\r\n                            resolve(window.serverData);\r\n                        });\r\n                });\r\n            }\r\n        }\r\n        \r\n    }\r\n}\r\n","D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\apiServices\\TransactionService.js",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\components\\Popup_Components\\SignInForm.component.jsx",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\components\\Popup_Components\\Form.component.jsx",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\components\\Popup_Components\\Input.component.jsx",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\components\\Popup_Components\\Offline.component.jsx",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\components\\Popup_Components\\Loading.component.jsx",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\\src\\components\\Popup_Components\\Error.component.jsx",[],"D:\\Tejas\\Work\\Repository\\MernPos\\Client\